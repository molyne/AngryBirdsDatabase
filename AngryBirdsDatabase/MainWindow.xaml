        <Label x:Name="PlayerListBoxLabel" Content="Players table:" HorizontalAlignment="Left" Margin="291,254,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label x:Name="LevelListboxLabel" Content="Levels table:" HorizontalAlignment="Left" Margin="505,254,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
